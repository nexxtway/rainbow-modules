{
    "name": "@rainbow-modules/firebase-hooks",
    "version": "0.53.0",
    "author": "Nexxtway Team",
    "license": "MIT",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json"
    },
    "peerDependencies": {
        "firebase": ">=8.2.0",
        "react": ">=16.8.0",
        "react-query": ">=3.24.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "./lib/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "module": "./lib/esm/index.js",
            "require": "./lib/index.js",
            "default": "./lib/esm/index.js"
        },
        "./lib/*": {
            "module": "./lib/esm/*",
            "require": "./lib/*",
            "default": "./lib/esm/*"
        }
    },
    "sideEffects": false
}
