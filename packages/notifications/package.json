{
    "name": "@rainbow-modules/notifications",
    "version": "0.51.1",
    "author": "Nexxtway Team",
    "license": "MIT",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json"
    },
    "dependencies": {
        "@rainbow-modules/hooks": "^0.51.1",
        "@rainbow-modules/icons": "^0.51.0",
        "styled-components": ">=4.3.2 <6"
    },
    "peerDependencies": {
        "firebase": ">=8.2.0",
        "prop-types": "^15.7.2",
        "react": ">=16.8.0",
        "react-intl": "^4.4.0",
        "react-rainbow-components": ">=1.15.0",
        "styled-components": ">=4.3.2 <6"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/cjs/index.d.ts",
    "exports": {
        ".": {
            "module": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js",
            "default": "./lib/esm/index.js"
        },
        "./*": {
            "module": "./lib/esm/*",
            "require": "./lib/cjs/*",
            "default": "./lib/esm/*"
        }
    },
    "sideEffects": false
}
