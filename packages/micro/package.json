        "lib"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json"
    },
    "dependencies": {
        "@rainbow-modules/icons": "^0.44.0",
        "styled-components": ">=4.3.2 <6"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
