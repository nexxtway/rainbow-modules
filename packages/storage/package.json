{
    "name": "@rainbow-modules/storage",
    "version": "0.54.0",
    "author": "Nexxtway Team",
    "license": "MIT",
    "files": [
        "lib"
    ],
    "dependencies": {
        "@rainbow-modules/firebase-hooks": "^0.54.0",
        "@rainbow-modules/icons": "^0.54.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-rainbow-components": ">=1.18.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "./lib/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "module": "./lib/esm/index.js",
            "require": "./lib/index.js",
            "default": "./lib/esm/index.js"
        },
        "./lib/*": {
            "module": "./lib/esm/*",
            "require": "./lib/*",
            "default": "./lib/esm/*"
        }
    },
    "sideEffects": false
}
